	<queryString language="SQL">
		<![CDATA[select a.* ,
(
	select d.nome from turma t
	join disciplina d on d.disciplina_id = t.disciplina_id
	where t.turma_id = $P{P_TURMA_ID} 
) nome_disciplina
from aluno a
where ($P{P_ALUNO_ID} is null or a.aluno_id =  $P{P_ALUNO_ID})
